#===> primary functions
DVFM_EVS_PRIMARYFUNCTIONS = dvfmEvsUmlGetCliErrorMessage\
							dvfmEvsUmlGetNcursesErrorMessage\
							dvfmEvsUmlGetWebErrorMessage\
							dvfmEvsUmlGetCliUserInterfaceMessage\
